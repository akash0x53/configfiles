# example config (see "hg help config" for more info)

[ui]
username = Akash Shende<akash0x53s@gmail.com>
editor=vim

[extensions]
pager =
progress =
color =
rebase =
strip=
mq=
histedit=
record=
shelve=
;glog=

[color]
mode = terminfo

[color "status"]
modified = blue bold 
added    = green bold
removed  = red bold 
deleted  = cyan bold

[color "diff"]
inserted = green

[color "branch"]
current = green
active  = None

[alias]
stat=status -m -a -r
annotate=annotate -c -u
graph=log --graph
push=push -b default
